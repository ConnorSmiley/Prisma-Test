generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Prisma {
  id BigInt @id @default(autoincrement())
}

model Projects{
  id Int @id @default(autoincrement())
  name String?
  Email Email[]
}

model Email {
  id Int @id @default(autoincrement())
  name String?
  DateTime DateTime?
  order Int?
  project Projects @relation(fields : [projectId], references: [id])
  projectId Int
}

